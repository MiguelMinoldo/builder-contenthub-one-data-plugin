// contents of .babelrc
// use preset-react to support JSX
{
  "presets": ["@babel/preset-react"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", { "development": true }],
        ["@babel/preset-typescript", { "development": true }],
        ["@babel/preset-env", { "development": true }]
      ]
    }
  }
}
